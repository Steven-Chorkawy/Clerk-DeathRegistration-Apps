{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "death-register-form-form-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/deathRegisterForm/DeathRegisterFormFormCustomizer.js",
          "manifest": "./src/extensions/deathRegisterForm/DeathRegisterFormFormCustomizer.manifest.json"
        }
      ]
    },
    "still-birth-form-form-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/stillBirthForm/StillBirthFormFormCustomizer.js",
          "manifest": "./src/extensions/stillBirthForm/StillBirthFormFormCustomizer.manifest.json"
        }
      ]
    },
    "registration-reports-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/registrationReports/RegistrationReportsWebPart.js",
          "manifest": "./src/webparts/registrationReports/RegistrationReportsWebPart.manifest.json"
        }
      ]
    },
    "monthly-death-reports-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/monthlyDeathReports/MonthlyDeathReportsWebPart.js",
          "manifest": "./src/webparts/monthlyDeathReports/MonthlyDeathReportsWebPart.manifest.json"
        }
      ]
    },
    "funeral-home-invoice-report-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/funeralHomeInvoiceReport/FuneralHomeInvoiceReportWebPart.js",
          "manifest": "./src/webparts/funeralHomeInvoiceReport/FuneralHomeInvoiceReportWebPart.manifest.json"
        }
      ]
    },
    "death-registration-display-form-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/deathRegistrationDisplayForm/DeathRegistrationDisplayFormCommandSet.js",
          "manifest": "./src/extensions/deathRegistrationDisplayForm/DeathRegistrationDisplayFormCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DeathRegisterFormFormCustomizerStrings": "lib/extensions/deathRegisterForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "StillBirthFormFormCustomizerStrings": "lib/extensions/stillBirthForm/loc/{locale}.js",
    "RegistrationReportsWebPartStrings": "lib/webparts/registrationReports/loc/{locale}.js",
    "MonthlyDeathReportsWebPartStrings": "lib/webparts/monthlyDeathReports/loc/{locale}.js",
    "FuneralHomeInvoiceReportWebPartStrings": "lib/webparts/funeralHomeInvoiceReport/loc/{locale}.js",
    "DeathRegistrationDisplayFormCommandSetStrings": "lib/extensions/deathRegistrationDisplayForm/loc/{locale}.js"
  }
}
